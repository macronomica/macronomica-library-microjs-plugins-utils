{
  "name": "@microjs/microjs-plugins-utils",
  "version": "0.1.0",
  "description": "Плагин Microjs с разными утилитными методами",
  "main": "index.js",
  "author": "Konstantin Dark",
  "scripts": {
    "dev:setup": "npm i && npm run dev:build",
    "dev:build": "npm run clean:build && gulp build",
    "dev:watch": "gulp watch",
    "prod:build": "npm run clean:release && gulp build:release",
    "clean:build": "rimraf ./build",
    "clean:release": "rimraf ./release",
    "clean:modules": "rimraf ./node_modules",
    "git:patch": "gulp patch",
    "git:feature": "gulp feature",
    "git:release": "gulp release",
    "test": "mocha -R spec --timeout 5000 test/index.js test/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/microjs/microjs-plugins-utils.git"
  },
  "dependencies": {
    "config": "^1.19.0",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.10",
    "dot-object": "^1.5.4",
    "lodash.uniq": "^4.5.0",
    "properties": "^1.2.1"
  },
  "peerDependencies": {
    "microjs": "git+ssh://git@gitlab.com:microjs/microjs.git"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^6.4.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-bump": "^2.1.0",
    "gulp-git": "^1.7.2",
    "gulp-sourcemaps": "2.0.1",
    "gulp-tag-version": "^1.3.0",
    "mocha": "^3.1.2",
    "semver": "^5.3.0",
    "rimraf": "^2.5.4"
  }
}
